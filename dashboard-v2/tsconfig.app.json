{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "types": ["vue-macros/macros-global", "vite/client"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": ["dom", "ES2023"],
    "strict": true
  },
  "vueCompilerOptions": {
    "plugins": ["vue-macros/volar"]
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"]
}
